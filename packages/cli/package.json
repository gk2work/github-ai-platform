{
  "name": "@github-ai/cli",
  "version": "1.0.0",
  "description": "Command line interface for GitHub AI Platform",
  "main": "dist/index.js",
  "bin": {
    "github-ai": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@github-ai/shared": "1.0.0",
    "@github-ai/core": "1.0.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.10",
    "ora": "^7.0.1",
    "chalk": "^5.3.0",
    "boxen": "^7.1.1",
    "table": "^6.8.1",
    "progress": "^2.0.3",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.0",
    "@types/inquirer": "^9.0.3",
    "@types/progress": "^2.0.5",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "eslint": "^8.44.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  }
}
